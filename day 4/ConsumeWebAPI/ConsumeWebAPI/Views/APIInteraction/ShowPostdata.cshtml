



<script>
    function GetPost()
    {
        fetch('https://jsonplaceholder.typicode.com/posts').then(result => result.json())
                                                           .then( data => {
                                                               // alert('Got data');
                                                               // console.log(data);
                                                               for(var i=0;i<data.length;i++)
                                                               {
                                                                   $("table").append("<tr> " + 
                                                                        "<td>" + data[i].id + "</td>" +
                                                                        "<td>" + data[i].userId + "</td>" +
                                                                        "<td>" + data[i].title + "</td>" +
                                                                        "<td>" + data[i].body + "</td>" +
                                                                   
                                                                   
                                                                   + "</tr>")
                                                               }

                                                            
                                                           })
                                                           .catch( errInfo =>{
                                                               //alert('Error !!');
                                                               console.log(errInfo);
                                                           })

    }
</script>


<button class="btn btn-primary" onclick="GetPost()">Show Post</button>

<table class="table table-bordered"></table>