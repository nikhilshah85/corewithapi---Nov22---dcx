

<script>
    function getPhotos()
    {
        fetch('https://jsonplaceholder.typicode.com/photos').then(data=> data.json())
                                                    .then(result => {
                                                        for(var i=0;i<result.length;i++)
                                                        {
                                                            $("table").append("<tr>" + 
                                                            
                                                                "<td>" + result[i].id + "</td>" +
                                                                "<td>" + result[i].albumId + "</td>" +
                                                                "<td>" + result[i].title + "</td>" +
                                                                "<td><img src=" + result[i].thumbnailUrl + "/></td>" +
                                                            
                                                            + "<tr/>")
                                                        }
                                                    })
                                                    .catch(err =>{
                                                        console.log(err);                                                    
                                                    });
    }
</script>

<button class="btn btn-warning" onclick="getPhotos()"> Show Photos data</button>
<table class="table table-striped"> </table>